# Frontend Environment Configuration Template
# Copy this to .env and update with your actual values

# Meeting Server Configuration
REACT_APP_MEETING_SERVER_URL=http://localhost:3001

# Main API Server Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Backend Server Configuration
REACT_APP_BACKEND_URL=http://localhost:5000

# Agora Configuration
REACT_APP_AGORA_APP_ID=your_agora_app_id_here

# Whiteboard Configuration
REACT_APP_WHITEBOARD_APP_ID=your_whiteboard_app_id_here

# Vite-specific Agora Configuration (for Vite builds)
VITE_AGORA_APP_ID=your_vite_agora_app_id_here
VITE_AGORA_CHAT_APP_KEY=your_chat_app_key_here
VITE_WHITEBOARD_APP_ID=your_whiteboard_identifier_here

# External Services
REACT_APP_UI_AVATARS_BASE_URL=https://ui-avatars.com/api